<?xml version="1.0" ?>

<launch>

	<param command="$(find xacro)/xacro $(find rvbot_description)/urdf/rvbot.xacro" name="robot_description"/>
	
	<node args="-param robot_description -urdf -model rvbot" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"/>
	
	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="world_name" value="$(find rvbot_gazebo)/worlds/rvbot_world.world"/>
		<arg name="paused" value="false"/>
		<arg name="use_sim_time" value="true"/>
		<arg name="gui" value="true"/>
		<arg name="headless" value="false"/>
		<arg name="debug" value="false"/>
	</include>

</launch>